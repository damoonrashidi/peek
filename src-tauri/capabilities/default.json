{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "core:window:default",
    "core:window:allow-start-dragging",
    "fs:default",
    "fs:allow-write-text-file",
    "fs:write-all",
    "fs:allow-desktop-write-recursive",
    "dialog:default"
  ]
}
